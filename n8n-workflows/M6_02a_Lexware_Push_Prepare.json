        "jsCode": "const input = $input.first().json;\nconst eventId = input.body?.event_id || input.event_id;\nconst projectId = input.body?.project_id || input.project_id;\nconst payload = input.body?.payload || input.payload || {};\nconst purchaseInvoiceId = payload.purchase_invoice_id || payload.invoice_id || payload.entity_id || input.body?.entity_id || input.body?.reference_id;\nconst stepKey = 'lexware_push_prepare:' + purchaseInvoiceId;\n\nreturn {\n  json: {\n    event_id: eventId,\n    project_id: projectId || null,\n    purchase_invoice_id: purchaseInvoiceId,\n    step_key: stepKey\n  }\n};"
      },
