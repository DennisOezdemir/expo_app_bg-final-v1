    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Has contact → emit LEXWARE_CONTACT_READY\nDO $$ BEGIN\n  UPDATE events SET processed_at = now()\n  WHERE id = '{{ $('Parse Event Data').item.json.event_id }}'::uuid;\nEXCEPTION WHEN invalid_text_representation THEN NULL;\nEND $$;\n\nINSERT INTO events (event_type, project_id, payload, source_system, source_flow, reference_table, reference_id)\nVALUES (\n  'LEXWARE_CONTACT_READY',\n  {{ $('Parse Event Data').item.json.project_id ? \"'\" + $('Parse Event Data').item.json.project_id + \"'::uuid\" : \"NULL::uuid\" }},\n  jsonb_build_object(\n    'purchase_invoice_id', '{{ $('Parse Event Data').item.json.purchase_invoice_id }}',\n    'lexware_contact_id', '{{ $json.lexware_contact_id }}'\n  ),\n  'n8n', 'M6_02a_Lexware_Push_Prepare',\n  'purchase_invoices', '{{ $('Parse Event Data').item.json.purchase_invoice_id }}'::uuid\n)\nRETURNING id, event_type::text;",
        "options": {}
      },
